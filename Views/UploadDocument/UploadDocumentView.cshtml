@{
    Layout = "~/Views/Shared/_LayoutPage2.cshtml";
    
}
<div class="container" ng-controller="UploadDocumentController">
    <form name="loginForm" novalidate>
        <!-- navbar top -->
        <div class="row">
            <!-- page header -->
            <div class="col-lg-12">
                <h1 class="page-header">Upload Document</h1>
            </div>
            <!--end page header -->
        </div>
        <div class="row">
            <div class="col-lg-12">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        Upload Document
                    </div>
                    <div class="panel-body">
                    </div>
                    <div class="panel-body">
                        <div class="row">
                            <div class="col-lg-6">
                                <div class="form-group">
                                    <label>Image</label>
                                    <input type="file" id="trigger" ng-model="PackagePhoto" onchange="angular.element(this).scope().setFile(this)" accept=".jpg">
                                </div>
                            </div>
                        </div>
                        {{ PackagePhoto }}
                    </div>
                    <input type="submit" class="btn-btn-lg btn-success " ng-click="" value="Submit" />
                </div>
            </div>
        </div>
    </form>
</div>
